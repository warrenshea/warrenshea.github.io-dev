                  <div class="image">
                    <div class="ic ic-type:solid display:inline-block ic-color:blue ic-size:72px ic-name:dragon"></div>
                  </div>
                  <div class="description">Card Checkbox Label 2</div>
                </label>

                <span class="error-message" id="card-checkbox-required:is_not_empty">Please select an option</span>
              </fieldset>
            </div>

            <div class="bgc:silver p:16px br-white:2px">
              <h3 class="fs:20px lh:24 text-align:center">Traditional Radio Buttons</h3>
            </div>

            <div class="form:theme:gl0b3x p:16px" data-equalize-height="traditional-radio-buttons">
              <fieldset>
                <legend>Traditional&nbsp;radio buttons *</legend>
                <input type="radio" name="traditional-radio-button-required" id="traditional-radio-button-required-1" value="1" data-required="is_not_empty">
                <label class="display:block py:4px" for="traditional-radio-button-required-1">Radio Button 1</label>
                <input type="radio" name="traditional-radio-button-required" id="traditional-radio-button-required-2" value="2">
