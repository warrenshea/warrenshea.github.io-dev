<%- include(`${process.env.PWD}/_dev/_layouts/_default.head.ejs`, pageData) %>
    <main class="w:100% form:theme:gl0b3x" role="main">
      <div class="mw:1280px align-self:centered sm+:px:8px xl+:px:0 pt:32px">
        <h1 class="play:bold fs:40px color:eerie-black sm+:px:8px xl+:px:0">Menu and Submenu</h1>

        <style>
          .x-men[aria-expanded=true]{background:#ff9999;}
          .avengers[aria-expanded=true]{background:#ff6666;}
          .justice-society[aria-expanded=true]{background:#9999ff;}
          .legion-of-doom[aria-expanded=true]{background:#bbbbff;}
        </style>
        <h2 class="play:bold fs:32px color:eerie-black py:32px">Vertical Menu and Submenu</h2>
        <ul role="menubar" id data-module="menu" data-menu-active="multiple" class="unstyle pl:0">
          <li role="none" style="width:600px">
            <button role="menuitem" class="cursor:pointer display:flex align-items:center b:0 color:white heebo:bold p:8px w:100% text-align:left bgc:red" data-module="menu.trigger" data-menu-open aria-controls aria-expanded tabindex>Favorite Marvel Heroes <span class="pl:32px ic ic-type:solid display:inline-block ic-color:white ic-size:16px ic-name:chevron-circle-right"></span></button>
            <div id data-module="menu.submenu" class="w:100% position:relative unstyle pl:0" data-equalize-height="marvel" style="background:lightpink;">
              <ul role="menubar" id data-module="menu" data-menu-active="single" class="unstyle pl:0">
                <li role="none" class="pl:16px py:4px">Spider-Man</li>
                <li role="none" class="pl:16px py:4px">Venom</li>
                <li role="none">
